{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
    {{#article}}
    <tr>
        <th>{{id}}</th>
        <td>{{title}}</td>
        <td>{{content}}</td>
    </tr>
    </tbody>
    {{/article}}
</table>
<a href="/articles/{{article.id}}/edit" class="btn btn-primary">수정하기</a>
<a href="/articles/{{article.id}}/delete" class="btn btn-primary">삭제하기</a>
<a href="/articles">전체 게시글 보기</a>
{{>layouts/footer}}
